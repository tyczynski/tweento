{
	"name": "tweento",
	"version": "1.0.1",
	"description": "Animation library based on CSS animations.",
	"keywords": [
		"animation",
		"transition",
		"trigger",
		"vanilla-javascript",
		"vanilla-js",
		"vanillajs"
	],
	"main": "lib/tweento.esm.js",
	"module": "lib/tweento.esm.js",
	"unpkg": "lib/tweento.iife.js",
	"files": [
		"lib",
		"src"
	],
	"scripts": {
		"clean": "rimraf lib",
		"build:esm": "rollup --config scripts/esm.js",
		"build:iife": "rollup --config scripts/iife.js",
		"build:js": "yarn build:esm && yarn build:iife",
		"build": "yarn clean && yarn build:js"
	},
	"homepage": "https://github.com/tyczynski/tweento.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tyczynski/tweento.js.git"
	},
	"bugs": {
		"url": "https://github.com/tyczynski/tweento.js/issues"
	},
	"licenses": [
		{
			"type": "MIT",
			"url": "https://github.com/tyczynski/tweento.js/blob/master/LICENSE"
		}
	],
	"author": {
		"name": "Przemysław Tyczyński",
		"url": "https://tyczynski.pl",
		"email": "przemyslaw@tyczynski.pl"
	},
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.8.6",
		"@babel/preset-env": "^7.8.6",
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"husky": "^4.2.3",
		"jest": "^25.1.0",
		"lint-staged": "^10.0.8",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.2",
		"rollup": "^1.32.0",
		"rollup-plugin-node-externals": "^2.1.3",
		"rollup-plugin-terser": "^5.2.0"
	},
	"dependencies": {
		"calliffn": "^1.0.1"
	}
}
